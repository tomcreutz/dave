        <pose>-0.20 0.70 -0.15 0 0 0</pose>
        <geometry>
          <box>
            <size>2.20 0.10 1.10</size>
          </box>
        </geometry>
      </collision>

      <collision name="right_side">
        <pose>0.20 -0.70 -0.15 0 0 0</pose>
        <geometry>
          <box>
            <size>2.20 0.10 1.10</size>
          </box>
        </geometry>
      </collision>
    </link>

    <link name="camera_link">
      <pose>1.33 0 -0.74 0 0 0</pose>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.000166667</ixx>
          <iyy>0.000166667</iyy>
          <izz>0.000166667</izz>
        </inertia>
      </inertial>
